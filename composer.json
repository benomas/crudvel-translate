{
    "name": "benomas/crudvel-translate",
    "description": "Free Google Translate API PHP Package Fork of stichoza/google-translate-php",
    "keywords": ["google", "translate", "translator", "php"],
    "license": "MIT",
    "homepage": "https://github.com/benomas/crudvel-translate",
    "authors": [
        {
            "name": "Levan Velijanashvili",
            "email": "me@stichoza.com"
        },
        {
            "name": "benomas",
            "email": "benomas@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "guzzlehttp/guzzle": "~7.0",
        "ext-json": "*",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Stichoza\\GoogleTranslate\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Stichoza\\GoogleTranslate\\Tests\\": "tests/"
        }
    }
}
